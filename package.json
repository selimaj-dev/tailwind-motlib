{
  "name": "tailwind-motlib",
  "version": "1.0.4",
  "description": "A motion and stagger library for tailwind",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js", 
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selimaj-dev/tailwind-motlib.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/selimaj-dev/tailwind-motlib/issues"
  },
  "homepage": "https://github.com/selimaj-dev/tailwind-motlib#readme",
  "devDependencies": {
    "@types/react": "^19.2.14",
    "typescript": "^5.9.3"
  }
}
